{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/analyze.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/server.js"
    },
    {
      "source": "/api/analyze",
      "destination": "/api/analyze.js"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ]
}
